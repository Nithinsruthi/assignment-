#include <stdio.h>
#include <string.h>

typedef struct stu1{
    char name1[6];
    char name2[6];
    double marks;
} STU1;

int main() {
    STU1 s1 = {"rohit", "kumar", 87.43};
    char *p = memchr(s1.name1, 'u', sizeof(s1.name1));
    if (p != NULL) {
        // null-terminate the string before printing
        *p = '\0';
        printf("%s\n", s1.name1);
    } else {
        printf("Not found\n");
    }
    return 0;
}
